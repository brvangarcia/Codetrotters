<h1>Users List</h1>
<a href="/users/create">Create</a>
<table border="1">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
      <tr>
        <td>{{firstName}}</td>
        <td>{{lastName}}</td>
        <td>{{id}}</td>
        <td><a href="/users/{{id}}/edit">Edit</a></td>
        <td><a href="/users/{{id}}/delete">Delete</a></td>
      </tr>
    {{/each}}
  </tbody>
</table>
