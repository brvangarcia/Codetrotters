<h1>Album List</h1>
<div style="margin: 0 auto;">
<table border="1" style="width: 80%;">
  <thead>
    <tr>
      <th>Id</th>
      <th>Artist</th>
      <th>Album</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
    <tr>
      <td>{{id}}</td>
      <td>{{artist}}</td>
      <td>{{album}}</td>
      <td>
        <a href="/albums/{{id}}">View</a>
        <a href="/albums/{{id}}/edits">Edit</a>
        <a href="">Delete</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
</div>
